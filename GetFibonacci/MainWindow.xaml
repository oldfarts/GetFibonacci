<Window x:Name="Fibonacci_UI1" x:Class="GetFibonacci.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GetFibonacci"
        mc:Ignorable="d"
        Title="Fibonacci UI" Height="450" Width="407.64" Icon="kg.jpg">
    <Grid x:Name="Fibonacci_UI">

        <Grid.Background>
            <ImageBrush ImageSource="kg.jpg"/>
        </Grid.Background>

        <!-- View report button -->
        <Button Margin="0,194,125,200" Width="152" Height="25" HorizontalAlignment="Right" Click="Button_Click">View</Button>
        <TextBlock Margin="0,229,125,99" Width="152" HorizontalAlignment="Right" Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" Name="textBlock"/>
        <TextBox Margin="0,94,125,230" Width="152" HorizontalAlignment="Right" Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" x:Name="textBlockInsert" AutomationProperties.HelpText="Enter Fibonacci value here..." AutomationProperties.AccessKey="7"/>
    </Grid>


</Window>
